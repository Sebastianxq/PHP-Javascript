                locManager.addString('ok.button', 'OK');
                locManager.addString('server.error.message', 'Your request could not be completed because of an error. Try again later.');
                locManager.addString('appleid.invalid', 'Enter a valid email address to use as your Apple ID.');
                locManager.addString('appleid.sameAsAppleID', 'This email address is not available. Choose a different address.');
                locManager.addString('appleid.appleOwnedDomain', 'Your Apple ID cannot end in {{placeHolder.domain}}. Choose a different email address.', false);
                locManager.addString('error.tooManyVerificationCode', 'A new code can’t be sent at this time. Enter the last code you received or try again later.');
                locManager.addString('captcha.empty', 'Enter the text in the image.');
                locManager.addString('web.signin.main.forgot.text', 'Forgot Apple ID or password?');
                locManager.addString('title.signin', 'Manage your Apple ID');
                locManager.addString('title.create', 'Create Your Apple ID');
                locManager.addString('link.privacy_letter', 'https:\/\/www.apple.com\/privacy\/');
                locManager.addString('title.faq', 'Apple ID FAQ');

                locManager.addString('ok.button.label', 'OK');

                locManager.addString('aria.digit', 'Digit');
                
                locManager.addString('web.manage.security.password.change.signout.text', 'Sign out of devices and websites using my Apple ID.');
                locManager.addString('web.manage.security.password.change.signout.ongoing.text', 'Signing out of devices and other browsers…');
